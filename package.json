{
  "name": "bedrock-native-app",
  "version": "1.0.0",
  "description": "Packages bedrock apps in native WebViews",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "create-app": "source ./native-app-config.sh && ./scripts/create.sh",
    "create-capacitor-config": "source ./native-app-config.sh && ./scripts/set-capacitor-config.sh",
    "set-app-manifests": "source ./native-app-config.sh && ./scripts/set-native-app-manifests.sh",
    "set-build-env": "source ./scripts/setup-build-env.sh",
    "generate-app-icons": "npx capacitor-assets generate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalbazaar/bedrock-native-app"
  },
  "dependencies": {
    "@capacitor/android": "^5.5.1",
    "@capacitor/app": "^5.0.6",
    "@capacitor/cli": "^5.5.1",
    "@capacitor/core": "^5.5.1",
    "@capacitor/ios": "^5.5.1"
  },
  "keywords": [
    "Bedrock",
    "Native",
    "App"
  ],
  "author": {
    "name": "Digital Bazaar, Inc.",
    "email": "support@digitalbazaar.com",
    "url": "https://digitalbazaar.com/"
  },
  "devDependencies": {
    "@capacitor/assets": "^2.0.4"
  }
}
